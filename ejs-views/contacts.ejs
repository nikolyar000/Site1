    .circle {
    position: relative;
    }
    .circle::before {
        content: "";
        position: absolute;
        width: 30vw;
        height: 30vw;
        border-radius: 50%;
        border: 8px solid #f9532d;
        opacity: .4;
    }
    .circle img:nth-child(2) {
        position: absolute;
        left: 0;
        top: 2rem;
        opacity: .4;
    }
    /* Section 1 End */
    
    .contact{
